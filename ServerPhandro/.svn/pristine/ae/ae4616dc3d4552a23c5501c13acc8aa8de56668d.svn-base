    public function WhoIs($ID, $Filed = null) {
        if (isset($ID)) {
            $Data = array(
                FILED_USER_ID => $ID
            );

            $tmp = $GLOBALS[CLASS_DATABASE]->select(TABLE_USER, $Data);
            $User = $GLOBALS[CLASS_TOOLS]->removeNull($tmp);
            if (!is_null($User)) {
                if (is_null($Filed)) {
                    return $User;
                } else {
                    return $User[$Filed];
                }
            } else {
                return null;
            }
        } else {
            return null;
        }
    }

    /**
     * Get_User_List
     * it is maybe geting with (Zone only) or (Network and Zone) or 
     * By (Country) or (Name) or (Barcode from UUID)
     */
    public function Get_User_List() {
        $Option = 0;
        $TableName = '';

        $User = array(
            FILED_PHONE_IMEI => $GLOBALS[CLASS_FILTER]->FilterData(KEY_PHONE_IMEI)
        );
        $PhoneNetwork = array(
            FILED_MOBILE_NETWORK_CODE => $GLOBALS[CLASS_FILTER]->FilterData(KEY_MOBILE_NETWORK_CODE),
            FILED_MOBILE_NETWORK_NETWORK_NAME => $GLOBALS[CLASS_FILTER]->FilterData(KEY_MOBILE_NETWORK_NETWORK_NAME),
            FILED_MOBILE_NETWORK_COUNTRY_NAME => $GLOBALS[CLASS_FILTER]->FilterData(KEY_MOBILE_NETWORK_COUNTRY_NAME)
        );
        $Country = array(
            FILED_COUNTRY_NAME => $GLOBALS[CLASS_FILTER]->FilterData(KEY_COUNTRY_NAME)
        );
        $UserName = array(
            FILED_USER_NAME => $GLOBALS[CLASS_FILTER]->FilterData(KEY_USER_NAME),
            FILED_USER_PHONE => $GLOBALS[CLASS_FILTER]->FilterData(KEY_USER_PHONE),
            FILED_USER_UUID => $GLOBALS[CLASS_FILTER]->FilterData(KEY_USER_UUID)
        );
        //For Get Users List depending on the $User ID Zone
        $User = $GLOBALS[CLASS_TOOLS]->removeNull($User);
        //For Ger Users List depending on the Network name in User Zone
        $PhoneNetwork = $GLOBALS[CLASS_TOOLS]->removeNull($PhoneNetwork);
        //For Get Users List depending on the Country which available to the user
        $Country = $GLOBALS[CLASS_TOOLS]->removeNull($Country);
        //For Get User from his Name or Phone or UUID
        $UserName = $GLOBALS[CLASS_TOOLS]->removeNull($UserName);

        if (!is_null($User)) {
            $Option = GET_BY_ZONE_ONLY;
        }

        if (!is_null($PhoneNetwork)) {
            if (in_array(FILED_MOBILE_NETWORK_NETWORK_NAME, $PhoneNetwork)) {
                if (in_array(FILED_MOBILE_NETWORK_COUNTRY_NAME, $PhoneNetwork)) {
                    $Option = GET_BY_NETWORK_AND_ZONE;
                } else {
                    $Option = GET_BY_NETWORK_ONLY;
                }
            } elseif (in_array(FILED_MOBILE_NETWORK_CODE, $PhoneNetwork)) {
                $Option = GET_BY_NETWORK_ONLY;
            }
        }

        if (!is_null($Country)) {
            $Option = GET_BY_COUNTRY;
        }

        if (!is_null($UserName)) {
            if (in_array(FILED_USER_NAME, $UserName)) {
                $Option = GET_BY_NAME;
            } elseif (in_array(FILED_USER_PHONE, $UserName)) {
                $Option = GET_BY_PHONE;
            } elseif (in_array(FILED_USER_UUID, $UserName)) {
                $Option = GET_BY_UUID;
            }
        }

        switch ($Option) {
            case GET_BY_ZONE_ONLY:
                return $this->Zone($User);
                break;
            case GET_BY_NETWORK_ONLY:
                return $this->Network($PhoneNetwork);
                break;
            case GET_BY_NETWORK_AND_ZONE:
                return $this->Network_With_Zone($PhoneNetwork);
                break;
            case GET_BY_COUNTRY:
                break;
            case GET_BY_NAME:
                break;
            case GET_BY_PHONE_TYPE:
                break;
            case GET_BY_UUID:
                break;
            default :
                break;
        }
    }

    private function Zone($User) {
        $Arrays_Users = array();
        $Users_List = array();
        $Zone_Phone = $GLOBALS[CLASS_DATABASE]->select(TABLE_PHONE, $User);
        if (!is_null($Zone_Phone)) {
            $UserID = $Zone_Phone[FILED_PHONE_UID];
            $Zone_Country = $Zone_Phone[FILED_PHONE_COUNTRY];
            $UserSex = $this->WhoIs($UserID, FILED_USER_SEX);

            $Where_Zone_Country = array(FILED_PHONE_COUNTRY => $Zone_Country);
            $Users_List_In_Phone_Table = $GLOBALS[CLASS_DATABASE]->select(TABLE_PHONE, $Where_Zone_Country);
            if (!is_null($Users_List_In_Phone_Table)) {

                foreach ($Users_List_In_Phone_Table as $Element_Array) {
                    if (is_array($Element_Array)) {
                        if ($Element_Array[FILED_PHONE_UID] != $UserID) {
                            array_push($Arrays_Users, $Element_Array[FILED_PHONE_UID]);
                        }
                    }
                }

                if (count($Arrays_Users) > 0) {
                    foreach ($Arrays_Users as $User_ID) {
                        $tmp = array(
                            FILED_USER_ID => intval($User_ID),
                            FILED_USER_ACTIVATION => ON
                        );
                        $Name = $GLOBALS[CLASS_DATABASE]->select(TABLE_USER, $tmp, '', 1, false, 'AND', FILED_USER_NAME . ',' . FILED_USER_SEX);
                        if (!is_null($Name)) {
                            if ($this->Get_Status_Line($User_ID)) {
                                $Name[FILED_USER_ONLINE_STATUS] = ON;
                            } else {
                                $Name[FILED_USER_ONLINE_STATUS] = OFF;
                            }
                            if (!empty($Name)) {
                                array_push($Users_List, $Name);
                            }
                        } else {
                            $this->LogERROR("You have Error in MySQL : " . $GLOBALS[CLASS_DATABASE]->ReturnError(), __FUNCTION__, __LINE__);
                        }
                    }

                    if (count($Users_List) > 0) {
                        //Sorting Sex
                        $The_Sime_Sex = array();
                        $The_Not_Sime_Sex = array();
                        $Key = 0;
                        foreach ($Users_List as $ElementUserList) {
                            if ($ElementUserList[FILED_USER_SEX] == $UserSex) {
                                $The_Sime_Sex[$Key] = $ElementUserList;
                            } else {
                                $The_Not_Sime_Sex[$Key] = $ElementUserList;
                            }
                            $Key++;
                        }

                        $Users_List = null;
                        $Users_List = array();
                        $Users_List = array_merge($The_Not_Sime_Sex, $The_Sime_Sex);

                        return $Users_List;
                    } else {
                        return FAIL . NOT_FOUND;
                    }
                }
                return FAIL . NOT_FOUND;
            } else {
                return FAIL . NOT_FOUND;
            }
        } else {
            $this->LogERROR("Not Found User " . $User[FILED_PHONE_IMEI] . " in Phone Table", __FUNCTION__, __LINE__);
            return FAIL . KEY_PHONE_IMEI;
        }
    }

    private function Network($PhoneNetwork_Code) {
        $Users_List = array();

        $Retern = $GLOBALS[CLASS_DATABASE]->select(TABLE_MOBILE_NETWORK, $PhoneNetwork_Code);
        if (!is_null($Retern)) {
            $NetworkID = array(
                FILED_PHONE_NETWORK => $Retern[FILED_MOBILE_NETWORK_ID]
            );

            $Users = $GLOBALS[CLASS_DATABASE]->select(TABLE_PHONE, $NetworkID);
            if (!is_null($Users)) {
                foreach ($Users as $element_user) {
                    if ($OwnerUserID != intval($element_user[FILED_PHONE_UID])) {
                        $UserID = array(FILED_USER_ID => intval($element_user[FILED_PHONE_UID]), FILED_USER_ACTIVATION => ON);
                        $Name = $GLOBALS[CLASS_DATABASE]->select(TABLE_USER, $UserID, '', 1, false, 'AND', FILED_USER_NAME . ',' . FILED_USER_SEX);

                        if (!is_null($Name)) {
                            if ($this->Get_Status_Line(intval($element_user[FILED_PHONE_UID]))) {
                                $Name[FILED_USER_ONLINE_STATUS] = ON;
                            } else {
                                $Name[FILED_USER_ONLINE_STATUS] = OFF;
                            }
                            if (!empty($Name)) {
                                array_push($Users_List, $Name);
                            }
                        } else {
                            $this->LogERROR("You have Error in MySQL : " . $GLOBALS[CLASS_DATABASE]->ReturnError(), __FUNCTION__, __LINE__);
                            return FAIL;
                        }
                    }
                }
                return $Users_List;
            } else {
                return NOT_FOUND;
            }
        } else {
            return NOT_FOUND;
        }
    }

