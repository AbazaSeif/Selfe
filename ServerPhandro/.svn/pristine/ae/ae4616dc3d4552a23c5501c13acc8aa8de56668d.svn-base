    function __set($name, $class) {
        if (is_null($name)) {
            if (class_exists($class)) {
                $name = new $class;
                return $name;
            } else {
                return null;
            }
        } else {
            return null;
        }
    }

    function __construct() {

        $this->UUID = (is_null($this->UUID = self::__set($this->UUID, UUID)) ? die(ShowError()) : $this->UUID);
